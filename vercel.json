{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/bot.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
